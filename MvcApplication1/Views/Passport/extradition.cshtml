<script src="@Url.Content("~/Scripts/jquery-1.8.2.js")"
        type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery-ui-1.8.24.js")"
        type="text/javascript"></script>
<script src="~/Scripts/jquery.unobtrusive-ajax.min.js" type="text/javascript"></script>
<link href="@Url.Content(
    "~/content/themes/base/jquery-ui.css")"
      rel="Stylesheet" type="text/css" />
<script type="text/javascript">
$(function () {
    var autocompleteUrl = '@Url.Action("Find")';
    var bla = $('#city').val();
    $("#city").autocomplete({
        source: autocompleteUrl,
        minLength: 1,
        select: function (event, ui) {
            /*alert("Selected " + ui.item.label);*/
            $: ajax({
                url: '@Url.Action("Find", "Passport")',
                data: bla = $('#city').value,
                success: function (data) {
                    $('#city').html(data);
                }
            })
        }  
  });
});
</script>
<h2>Cities</h2>
<p>
    Start typing a city to see
    the autocomplete behavior in action.
</p>
<p>
    <label for="city">City</label>
    <input type="text" id="city"/>
</p>

